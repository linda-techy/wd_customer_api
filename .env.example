# Copy to .env and fill with your values
# APP_ENV= (empty for dev), staging, or production

# Database
DB_URL=jdbc:postgresql://localhost:5432/wdTestDB?connectTimeout=10&socketTimeout=30&tcpKeepAlive=true&loginTimeout=10&cancelSignalTimeout=10
DB_USERNAME=postgres
DB_PASSWORD=your_password

# JWT (use a secure secret in production)
JWT_SECRET=your_jwt_secret_hex
JWT_ACCESS_EXPIRATION=3600000
JWT_REFRESH_EXPIRATION=604800000

# Storage path for file uploads
STORAGE_BASE_PATH=uploads

# Optional: override server port
# SERVER_PORT=8080
